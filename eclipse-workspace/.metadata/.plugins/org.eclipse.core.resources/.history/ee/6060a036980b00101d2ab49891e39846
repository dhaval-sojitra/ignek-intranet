package com.ignek.intranet.count.of.configuration;

import com.liferay.configuration.admin.category.ConfigurationCategory;

import org.osgi.service.component.annotations.Component;

@Component(service = ConfigurationCategory.class)
public class CountOfCategoryCategory implements ConfigurationCategory {

	@Override
	public String getCategoryKey() {
		return _CATEGORY_KEY;
	}

	@Override
	public String getCategorySection() {
		return _CATEGORY_SECTION;
	}

	@Override
	public String getCategoryIcon() {
		return "user";
	}

	private static final String _CATEGORY_KEY = "countOf";

	private static final String _CATEGORY_SECTION = "countOf";

}
